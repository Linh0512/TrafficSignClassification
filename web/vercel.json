{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "MODEL_PATH": "best.pt"
  },
  "functions": {
    "api/app.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
} 