{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "MODEL_PATH": "best_yolo12.pt"
  },
  "functions": {
    "api/index.py": {
      "memory": 3008,
      "maxDuration": 30
    }
  }
} 